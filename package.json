{
  "name": "ra-datagrid-sortable",
  "version": "1.0.0",
  "description": "Drag'n'drop sortable datagrid for react-admin",
  "files": [
    "*.md",
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "repository": "github.com:guillemfondin/ra-datagrid-sortable.git",
  "author": "Guillem Fondin <guillem.fondin@hotmail.fr>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
    "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
    "watch": "tsc --outDir dist/esm --module es2015 --watch"
  },
  "devDependencies": {
    "@mui/material": "^5.0.2",
    "@types/sortablejs": "^1.15.0",
    "ra-core": "^4.5.2",
    "ra-ui-materialui": "^4.5.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "typescript": "^4.4.0"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.2",
    "ra-core": "^4.5.2",
    "ra-ui-materialui": "^4.0.0",
    "react": "^16.9.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.9.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react-sortablejs": "^6.1.4",
    "sortablejs": "^1.15.0"
  }
}
